package StringS;

import java.util.Arrays;

public class printsepratestringnnumber 
{

	public static void main(String[] args) 
	{
	    String s="Arjun 77759";
	    char[] w = s.toCharArray();
	    Arrays.sort(w);
	    for(int i=w.length-1;i>=0;i--)
	    {
	    	System.out.println(s.charAt(i));
	    }

	}

}
