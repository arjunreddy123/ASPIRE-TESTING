import java.util.Scanner;

public class nested_ifelse 
{

	public static void main(String args[]) 
	{
		int num1, num2, num3;
		
		Scanner s=new Scanner(System.in);
		num1=s.nextInt();
		num2=s.nextInt();
		num3=s.nextInt();
		
		if(num1>num2)
     {
    	 if(num1>num3)
    	 {
    		System.out.print("maximum no:"+num1);
    	 }
    	 else
    	 {
    		System.out.print("MAXIMUM NO:"+num3);
    	 }
     }
  
	}

}
