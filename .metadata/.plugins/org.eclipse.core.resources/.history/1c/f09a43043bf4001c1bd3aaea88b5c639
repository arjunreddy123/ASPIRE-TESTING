

public class nested_ifelse 
{

	public static void main(String args[]) 
	{
		int num1=20, num2=10, num3=45;
		
		if(num1>num2)
     {
    	 if(num1>num3)
    	 {
    		System.out.print("maximum no:"+num1);
    	 }
    	 else
    	 {
    		System.out.print("MAXIMUM NO:"+num3);
    	 }
     }
  
	}

}
