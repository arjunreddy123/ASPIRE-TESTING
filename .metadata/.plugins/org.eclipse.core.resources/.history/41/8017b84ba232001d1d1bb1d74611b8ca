package Practicepack;

public class constructor 
{
	int a;int b,c;
	constructor (int y)
	{
		this.a=y;
	}
	constructor (int x,int z)
	{
		c=z+x;
	}
	public void show() 
	{
		//System.out.println();
		System.out.println(a);
		System.out.println(c);
	}
	public static void main(String[] args) 
	{
		constructor c=new constructor (200);
		constructor s=new constructor (100,200);
		//c.a=500;
		//c.c=800;
		c.show();
		s.show();

	}

}
