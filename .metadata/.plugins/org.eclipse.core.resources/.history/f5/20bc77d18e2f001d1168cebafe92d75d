package Findelements;

import java.util.List;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;

public class amazon 
{

	public static void main(String[] args) 
	{
		String key="webdriver.chrome.driver";
		String value="C:\\Users\\arnav\\OneDrive\\Desktop\\JAVA PROJECT\\June2022_Selenium\\Driver\\chromedriver.exe";
        String url="https://www.amazon.in/?&ext_vrnc=hi&tag=googinhydr1-21&ref=pd_sl_6vkm4swd5x_b&adgrpid=60611463244&hvpone=&hvptwo=&hvadid=617724335923&hvpos=&hvnetw=g&hvrand=1747770768484171508&hvqmt=b&hvdev=c&hvdvcmdl=&hvlocint=&hvlocphy=9040233&hvtargid=kwd-320833120261&hydadcr=15413_2322997&gclid=Cj0KCQjwpeaYBhDXARIsAEzItbFrTD0lfo5xboEHd9IsIxCmz6-nZBZ1nm-7Pf04WYlQ8lDVR11S_rcaAu4ZEALw_wcB";
        System.setProperty(key, value);
        WebDriver driver=new ChromeDriver();
        driver.manage().window().maximize();
        driver.get(url);
        List<WebElement> list = driver.findElements(By.xpath("//*[@id=\"nav-xshop\"]/a"));
        for(WebElement element:list)
        {
        	System.out.println(element.getText());
        }

	}

}
