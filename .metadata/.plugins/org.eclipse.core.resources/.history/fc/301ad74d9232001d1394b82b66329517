package Array;

public class Names 
{

	public static void main(String[] args) 
	{
		    String[] Name = new String[100];
		    Name[0]="Arjun";
		    Name[1]="Arnav";
		    Name[2]="Aryan";
		    Name[99]="Akash";
		
		    System.out.println(Name[99]);
		    System.out.println(Name[00]);
		 
		    //useing Ascii 
		    int[] v= new int[5];
			v[0]='A';
			v[1]='R';
			v[2]='J';
			v[3]='U';
			v[4]='N';
			
		    System.out.println("Ascii Value Is A="+v[0]);
		
		    int[] vipno = {111, 222, 333, 444, 555};
		 
		    System.out.println("Accessing Elements of Array:");
		    System.out.println("First Element:"+vipno[0]);
		    System.out.println("Second Element:"+vipno[1]);
		    System.out.println("Third Element:"+vipno[2]);
		    System.out.println("Fourth Element:"+vipno[3]);
		    System.out.println("Fifth Element:"+vipno[4]);
		   

		   
		  
		  
		    // create an array                                       // loop through the array
		    int[] age = {12, 4, 5,7,9};                                 // using for loop
		    System.out.println("Using for Loop:");
		    for(int i = 0; i <age.length; i++) 
		    {
		     System.out.print(age[i]+",");
	        }
		   
		    // loop through the array
		    // using for each loop
		    System.out.println();
		    int[] age1 = {12, 4, 5,7,9};
		   
            System.out.println("Using for-each Loop:");
		    for(int a :age1) 
		    {
		     System.out.print(a+",");
		    }

	}
}
